class Solution:
    def queensAttacktheKing(self, queens: List[List[int]], king: List[int]) -> List[List[int]]:
        queen.sort()
        self.queens=queens
        self.king=self.king
        out=0
        for queen in queens:
            if self.check(queen):
                out+=1
        return out
    
